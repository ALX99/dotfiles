! URXVT stuff
urxvt.depth: 32
URxvt.background: [70]black
urxvt.font: xft:Noto Sans Mono:style=Regular:pixelsize=14, 4xft:FontAwesome5Free:style=Solid:pixelsize=14:antialias=true
urxvt.letterSpace: -1
urxvt.scrollBar:	false
urxvt.cursorColor:	green
urxvt.cursorBlink:	true
urxvt.cursorUnderline:	1
urxvt.url-launcher: 	/usr/bin/chromium
urxvt.perl-ext-common: resize-font,matcher,default
urxvt.matcher.button:	1
urxvt.matcher.rend.0: Uline Bold fg3
urxvt.keysym.C-n: resize-font:bigger
urxvt.keysym.C-e: resize-font:smaller
urxvt.keysym.C-i: resize-font:reset
